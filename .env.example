# k3ss-IDE Environment Configuration

# API Keys for LLM Providers
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
GOOGLE_API_KEY=your-google-api-key-here
DEEPINFRA_API_KEY=your-deepinfra-api-key-here

# Ollama Configuration
OLLAMA_API_URL=http://localhost:11434

# Custom LLM Endpoint
CUSTOM_LLM_ENDPOINT=https://api.customllm.dev/v1

# Helicone API Key for Cost Tracking
HELICONE_API_KEY=your-helicone-api-key-here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Context Window Monitor Settings
MAX_CONTEXT_SIZE=16000
WARNING_THRESHOLD=0.8

# Agent Sidecar Configuration
INTERPRETER_PATH=/path/to/interpreter
INTERPRETER_ARGS=--verbose

# Application Settings
LOG_LEVEL=info
DEBUG_MODE=false
